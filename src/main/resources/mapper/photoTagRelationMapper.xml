<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.newbee.smart_album.dao.mapper.PhotoTagRelationMapper">

    <insert id="insert">
        insert into photo_tag_relation (photo_id,tag_id) values (#{photoId},#{tagId})
    </insert>

    <select id="selectPhotoIdByTagId" resultType="INTEGER">
        select photo_id from photo_tag_relation where tag_id=#{tagId}
    </select>
</mapper>